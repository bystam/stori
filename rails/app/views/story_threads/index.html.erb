<h1>Listing story_threads</h1>

<table>
  <tr>
    <th>Start</th>
    <th>End</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @story_threads.each do |story_thread| %>
  <tr>
    <td><%= story_thread.start %></td>
    <td><%= story_thread.end %></td>
    <td><%= story_thread.name %></td>
    <td><%= link_to 'Show', story_thread %></td>
    <td><%= link_to 'Edit', edit_story_thread_path(story_thread) %></td>
    <td><%= link_to 'Destroy', story_thread, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Story thread', new_story_thread_path %>
